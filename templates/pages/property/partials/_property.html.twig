<div class="card mb-4">
    <div class="card-body">
        <h5 class="card-title">
            <a href="{{ path('property_show',{slug:property.slug, id:property.id})}}">
                {{property.title}}
            </a>
        </h5>
        <p class="card-text">{{property.surface}}m2 -{{property.city}} {{property.postalCode}}</p>
        <div class="text-primary price-config">{{property.formattedPrice}}</div>
    </div>
    <p class=""></p>

</div>